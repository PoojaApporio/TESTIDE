<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="oval">
            <size android:width="40dp"
                android:height="40dp"/>
            <corners
                android:bottomRightRadius="5dp"/>
            <solid android:color="@color/icons_8_muted_grey"/>
            <padding android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp"/>
            <stroke android:color="@color/pure_black"
                android:width="1dp"/>
        </shape>
    </item>
    <item android:drawable="@drawable/ic_xl_suv"
        android:width="30dp"
        android:height="20dp"
        android:gravity="center" />
</layer-list>